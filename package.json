{
  "name": "bunnygod",
  "version": "0.1.0",
  "description": "An all-powerful living AI deity that answers philosophical questions",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "typecheck": "astro check",
    "lint": "eslint src --ext .ts,.tsx,.astro",
    "lint:fix": "eslint src --ext .ts,.tsx,.astro --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,astro,json,md}\"",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "worker:dev": "wrangler dev workers/ask/index.ts",
    "worker:deploy": "wrangler deploy workers/ask/index.ts"
  },
  "dependencies": {
    "@astrojs/react": "^3.6.2",
    "@astrojs/tailwind": "^5.1.1",
    "astro": "^4.15.12",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwindcss": "^3.4.14"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.6",
    "@cloudflare/workers-types": "^4.20241127.0",
    "@types/bun": "latest",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^8.13.0",
    "eslint": "^9.14.0",
    "eslint-plugin-astro": "^1.2.4",
    "prettier": "^3.3.3",
    "prettier-plugin-astro": "^0.14.1",
    "typescript": "^5.6.3",
    "wrangler": "^3.87.0"
  },
  "keywords": [
    "ai",
    "philosophy",
    "philpapers",
    "astro",
    "cloudflare",
    "typescript"
  ],
  "author": "Daniel Miessler",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/bunnygod.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/bunnygod/issues"
  },
  "homepage": "https://github.com/yourusername/bunnygod#readme"
}
